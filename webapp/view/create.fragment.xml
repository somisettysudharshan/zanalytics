<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   xmlns:t="sap.ui.table"
	xmlns:f="sap.ui.layout.form" >
   <Dialog id="SDialog" class="sapUiSizeCozy" resizable="true" draggable="true" title="System Error Report" contentHeight="400px"
		contentWidth="230px">
		<ScrollContainer height="100%" width="100%"
			horizontal="true" vertical="false">
			<f:SimpleForm minWidth="1024" maxContainerCols="2"
				layout="ResponsiveGridLayout">


				<t:Table id="tblsysTable" class="sapUiSizeCompact" selectionMode="None"
					editable="false" rows="{oModel>/d/Supplier/}" >


					<t:columns>
						<t:Column width="11rem" tooltip="Sender Component ">
							<Label text="Sender Component" />
							<t:template>
								<Text text="{oModel>/d/Supplier/Name}" />
							</t:template>
						</t:Column>

						<t:Column width="11rem" tooltip="Sender Interface Namespace "> <Label text="Sender Interface Namespace "/> 
							<t:template> <Text text="{oModel>/d/Supplier/Id}" /> </t:template> </t:Column>

						<t:Column width="11rem" tooltip="Sender Interface Name">
							<Label text="Sender Interface Name" />
							<t:template>
								<Text text="{oModel>/d/Supplier/Phone}" />
							</t:template>
						</t:Column>
						
							<t:Column width="11rem" tooltip="Receiver Component ">
							<Label text="Receiver Component" />
							<t:template>
								<Text text="{oModel>/d/Supplier/Email}" />
							</t:template>
						</t:Column>

			<t:Column width="11rem" tooltip="Receiver Interface Namespace">
							<Label text="Receiver Interface Namespace" />
							<t:template>
								<Text text="{oModel>/d/Supplier/WebAddress}" />
							</t:template>
						</t:Column>
						<t:Column width="11rem" tooltip="Receiver Interface Name ">
							<Label text="Receiver Interface Name" />
							<t:template>
								<Text text="{oModel>/d/Supplier/FormattedAddress}" />
							</t:template>
						</t:Column>
						<t:Column width="7rem" tooltip="Count"> <Label text="Count" /> <t:template> 
							<Text text="{oModel>/d/Supplier/ContactPhone1}" /> </t:template> </t:Column>

						
					</t:columns>
				</t:Table>

			</f:SimpleForm>
		</ScrollContainer>
		<beginButton>
			<!-- <Button text="OK" press="onSelectTransfer" /> -->
			<!-- <Button text="OK" press="onSelectTransferUpdate" /> -->

		</beginButton>
		<endButton>
			<Button text="Close" press="onCloseAppDialog" />
		</endButton>


	</Dialog>
</core:FragmentDefinition>